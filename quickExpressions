//returns the current time in seconds starting from 1
Math.floor(time)+1

//randomly sets a discrete value of either v1 or v2 at a frequency of f times per second
f = 5; //frequency
v1 = 0; //value 1
v2 = 100; //value 2
seedRandom(0, true); //random seed

posterizeTime(f);
r = random();
if (r < 0.5) {
	v1
} else {
	v2
};
